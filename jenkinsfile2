
pipeline {
    agent any

    stages {
        
        stage(maven) {
            steps{
                    sh 'mvn clean install -DskipTests'
                }
            }
        stage('Docker') {
            steps {
                  sh 'docker build -t realtime .'
                }
            
               } 
        stage('Docker tag') {
            steps {
                  sh 'docker tag realtime newtag .'
            }
        }


        }
}
     
              
